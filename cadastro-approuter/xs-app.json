{
  "welcomeFile": "app/index.html",
  "authenticationMethod": "none",
  "routes": [
    {
      "source": "^/api/(.*)$",
      "target": "$1",
      "destination": "api_house",
      "csrfProtection": false
    },
    {
      "source": "^/app(.*)$",
      "target": "$1",
      "authenticationType": "none",
      "localDir": "app"
    }
  ]
}